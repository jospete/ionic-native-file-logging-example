{
  "name": "ionic-native-file-logging-example",
  "version": "0.0.1",
  "description": "An example mobile app project to demonstrate native file logging.",
  "scripts": {
    "init": "run-s build cap:init trap",
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0 --port=8100",
    "trap": "trapeze run trapeze.yaml -y --android-project android --ios-project ios/App",
    "build": "ng build",
    "cap:init": "run-s cap:init:ios cap:init:android",
    "cap:init:ios": "cap add ios",
    "cap:init:android": "cap add android",
    "cap:sync": "cap sync",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~16.2.12",
    "@angular/core": "~16.2.12",
    "@angular/forms": "~16.2.12",
    "@angular/platform-browser": "~16.2.12",
    "@angular/platform-browser-dynamic": "~16.2.12",
    "@angular/router": "~16.2.12",
    "@awesome-cordova-plugins/core": "6.3.0",
    "@awesome-cordova-plugins/file": "6.3.0",
    "@awesome-cordova-plugins/social-sharing": "6.3.0",
    "@capacitor/android": "5.5.1",
    "@capacitor/core": "5.5.1",
    "@capacitor/filesystem": "5.1.4",
    "@capacitor/ios": "5.5.1",
    "@capacitor/splash-screen": "5.0.6",
    "@ionic/angular": "6.5.3",
    "@ionic/core": "6.5.3",
    "@obsidize/rx-console": "6.1.6",
    "cordova-plugin-secure-logger": "1.4.8",
    "cordova-plugin-x-socialsharing": "6.0.4",
    "es6-promise-plugin": "^4.2.2",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular/cli": "~16.2.10",
    "@angular/compiler": "~16.2.12",
    "@angular/compiler-cli": "~16.2.12",
    "@angular/language-service": "~16.2.12",
    "@capacitor/cli": "^4.8.1",
    "@ionic/angular-toolkit": "^7.0.0",
    "@ionic/lab": "3.2.10",
    "@trapezedev/configure": "7.0.10",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "npm-run-all": "4.1.5",
    "protractor": "^7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.9.5"
  }
